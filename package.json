{
  "name": "ultimate-comparison",
  "version": "2.0.0-alpha.32",
  "scripts": {
    "data:prepare": "node ./node_modules/gulp/bin/gulp.js default --gulpfile=./lib/gulp/gulpfile.js",
    "data:watch": "node ./node_modules/gulp/bin/gulp.js dev --gulpfile=./lib/gulp/gulpfile.js",
    "start": "npm run data:prepare && ng serve",
	"md2build" : "MD_TO_JSON_COMMAND=\"python ../../md2json/md2json.py\" npm run build",
    "dev": "concurrently \"npm run data:watch\" \"ng serve\"",
    "build": "npm run data:prepare && ng build",
    "build:prod": "npm run data:prepare && ng build --configuration production",
    "test": "ng test",
    "release": "npm run build:prod"
  },
  "files": [
    "dist",
    "configuration",
    "data",
    "LICENSE",
    "bin",
    "lib"
  ],
  "bin": {
    "uc": "./lib/uc/uc"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ultimate-comparisons/ultimate-comparison-BASE.git"
  },
  "babel": {
    "presets": ["@babel/preset-env"]
  },
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/common": "^17.3.8",
    "@angular/compiler": "^17.3.8",
    "@angular/core": "^17.3.8",
    "@angular/forms": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/router": "^17.3.8",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@ngrx/router-store": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "angular-http-server": "^1.9.0",
    "citation-js": "^0.7.11",
    "concurrently": "^8.2.2",
    "gulp": "^4.0.2",
    "gulp-exec": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.8",
    "@angular/language-service": "^17.3.8",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.11.30",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "patch-package": "^8.0.0",
    "typescript": "~5.2.2"
  }
}
