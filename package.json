{
  "name": "awesome-comparisons-v3",
  "version": "3.0.0-251029",
  "scripts": {
    "md2json:compile": "tsc -p ./lib/md2json/tsconfig.json",
    "md2json:run": "node ./lib/md2json/dist/cli.js",
    "md2json:run:default": "npm run md2json:compile && node ./lib/md2json/dist/cli.js --input ./data --tmp ./tmp/md2json --output ./dist/data.json",
    "data:prepare": "npm run md2json:compile && node ./node_modules/gulp/bin/gulp.js default --gulpfile=./lib/gulp/gulpfile.js",
    "data:watch": "npm run md2json:compile && node ./node_modules/gulp/bin/gulp.js dev --gulpfile=./lib/gulp/gulpfile.js",
    "env:generate": "node scripts/generate-environment.js",
    "start": "node ./scripts/run-start.js",
    "config:serve": "node ./node_modules/gulp/bin/gulp.js config-workspace:serve --gulpfile=./lib/gulp/gulpfile.js",
    "dev": "node ./scripts/run-dev.js",
    "prebuild": "rm -f tmp/*/comparison-auto-config.yml",
    "prebuild:prod": "npm run prebuild && npm run env:generate",
    "build": "node ./scripts/run-build.js",
    "build:prod": "node ./scripts/run-build.js --configuration production",
    "release:stamp": "node ./node_modules/gulp/bin/gulp.js release --gulpfile=./lib/gulp/gulpfile.js",
    "release:prepare": "npm run build:prod && npm run release:stamp",
    "templates:export": "node ./lib/comparison-template/export-all-templates.js",
    "test": "ng test",
    "release": "npm run release:prepare",
    "screenshots:track-a": "node scripts/capture-screenshots-track-a.js",
    "screenshots:track-b": "node scripts/capture-screenshots-track-b.js",
    "screenshots:track-c": "node scripts/capture-screenshots-track-c.js",
    "screenshots:track-d": "node scripts/capture-screenshots-track-d.js",
    "screenshots:all": "npm run screenshots:track-a && npm run screenshots:track-b && npm run screenshots:track-c && npm run screenshots:track-d",
    "lint": "ng lint"
  },
  "files": [
    "dist",
    "configuration",
    "data",
    "LICENSE",
    "lib"
  ],
  "bin": {
    "awcmp-cli": "./lib/awcmp-cli/awcmp-cli"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:cbruyndoncx/awesome-comparisons.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.2",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@ngrx/router-store": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "angular-http-server": "^1.9.0",
    "concurrently": "^8.2.2",
    "diff": "^5.1.0",
    "diff2html": "^3.4.47",
    "gulp": "^4.0.2",
    "gulp-exec": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "highlight.js": "^11.9.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "xlsx": "^0.18.5",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.2",
    "@angular-eslint/builder": "^18.1.0",
    "@angular-eslint/eslint-plugin": "^18.1.0",
    "@angular-eslint/eslint-plugin-template": "^18.1.0",
    "@angular-eslint/schematics": "^18.1.0",
    "@angular-eslint/template-parser": "^18.1.0",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@angular/language-service": "^21.0.3",
    "@types/diff": "^7.0.2",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "^8.57.0",
    "gh-pages": "^6.3.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "patch-package": "^8.0.0",
    "playwright-core": "^1.56.1",
    "sass": "^1.79.0",
    "typescript": "~5.9.3"
  }
}
